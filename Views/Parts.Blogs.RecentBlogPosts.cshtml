
@{
    IEnumerable<object> posts =
        Model.ContentItems.ContentItems;
}

<ul>
    @{
        var count = 0;
        foreach (dynamic post in posts)
        {
            var blogPostPart = post.ContentItem.BlogPostPart;
            <li>
                <a href=""><img class="left" width="50px" height="50px" src="@Url.Content((string)blogPostPart.FeaturedThumbnail.Url)" alt="@blogPostPart.Title"/></a>
            </li>
       
            count++;
        }
    }
</ul>